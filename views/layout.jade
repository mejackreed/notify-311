!!!
html(ng-app="myApp")
	head
		meta(charset='utf8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no')
		base(href='/')
		title Notify 311
		link(rel='stylesheet', href='//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.no-icons.min.css')
		link(rel='stylesheet', href='//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.min.css')
		link(rel="stylesheet", href="http://cdn.leafletjs.com/leaflet-0.6.2/leaflet.css")
		link(rel='stylesheet', href='/css/app.css')

		<!--[if lte IE 8]>
		<link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet-0.6.2/leaflet.ie.css" />
		<![endif]-->

	body(ng-controller='MapHomeCtrl')
		div.navbar.navbar-fixed-top
			div.navbar-inner
				div.container-fluid
					a(data-toggle='collapse', data-target='.nav-collapse').btn.btn-navbar
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a(href='#').brand Infra-Voter
					div.nav-collapse
						ul.nav
							block navbar
							

		div
			block body

		block scripts

		script(src='//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js')
		script(src='//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/js/bootstrap.min.js')
		script(src='http://code.angularjs.org/1.0.7/angular.min.js')
		script(src='/js/ui-bootstrap.js')
		script(src='http://code.angularjs.org/1.0.7/angular-resource.min.js')
		script(src='http://code.angularjs.org/1.0.7/angular-sanitize.min.js')
		script(src='/js/app.js')
		script(src='/js/controllers.js')
		script(src='/js/directives.js')
		script(src='/js/filters.js')
		script(src='/js/services.js')
